{{#md-navbar name='Dashboard' }}
  {{#each model as |ui index|}}

    {{#if (eq ui.engine true)}}
      {{#link-to "ember-stack-builder-engine" tagName="a" invokeAction=(action 'selectUi' ui index 'engine')}}
        {{ui.label}}
      {{/link-to}}
    
    {{else}}
      <li class="{{if (eq ui.id activeId) 'active'}}" {{action "selectUi" ui index 'iframe'}}>
        <a>{{ui.label}}</a>
      </li>
    {{/if}}
  {{/each}}
{{/md-navbar}}

{{#if activeUi}}
  <div class="frame-wrapper">
    {{#each model as |ui|}}
      {{render-ui activeUi=activeUi ui=ui classes=(concat directionClass " " ui.repoName)}}
    {{/each}}
  </div>
{{else}}
  <div class="col s12 light center header">
    <h2>Welcome to UI Integator</h2>
    <p>To get started select one of the items in the header.</p>
  </div>
{{/if}}
 
{{outlet}}

{{md-modal-container}}
